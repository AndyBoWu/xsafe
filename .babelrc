{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "88"
        },
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": "3"
      }
    ]
  ],
  "plugins": [],
  "env": {
    "development": {
      "sourceMaps": true,
      "retainLines": true
    },
    "production": {
      "sourceMaps": false,
      "comments": false
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
